namespace VarFishServerWorker;

// Enum for storing the SV type.
enum SvType : byte {
  Del,
  Dup,
  Inv,
  Ins,
  Bnd,
  Cnv,
}

// Background database record.
struct BgDbRecord {
  chrom_no: ubyte;
  chrom_no2: ubyte;
  sv_type: SvType;
  begin: uint;
  end: uint;
  count: uint;
}

// Background database consists of a Vector of BgDbRecords.
table BackgroundDatabase {
  records:[BgDbRecord];
}
